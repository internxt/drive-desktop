{
  "name": "internxt-drive",
  "version": "2.5.2",
  "author": "Internxt <hello@internxt.com>",
  "description": "Internxt client UI",
  "main": "./dist/main/main.js",
  "license": "AGPL-3.0",
  "repository": {
    "url": "https://github.com/internxt/drive-desktop"
  },
  "scripts": {
    "build": "concurrently \"npm run build:main\" \"npm run build:backups\" \"npm run build:renderer\" \"npm run build:sync-engine\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config .erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config .erb/configs/webpack.config.renderer.prod.ts",
    "build:sync-engine": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config .erb/configs/webpack.config.sync-engine.ts",
    "build:backups": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config .erb/configs/webpack.config.backups.ts",
    "build:dll": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config .erb/configs/webpack.config.renderer.dev.dll.ts",
    "package": "ts-node .erb/scripts/clean.js && npm run build && electron-builder build --publish never",
    "init:dev": "pnpm install && node node_modules/electron/install.js && npm run build:dll && npm run reload-native-deps",
    "========== Code style ==========": "",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 196",
    "lint:fix": "npm run run lint --fix",
    "format": "prettier src --check",
    "format:fix": "prettier src --write",
    "type-check": "tsc --noEmit",
    "========== Start ==========": "",
    "start": "npm run start:renderer",
    "start:nodemon": "nodemon",
    "start:reload": "npm run start:processes && npm run start:main",
    "start:processes": "concurrently \"npm run start:sync-engine\" \"npm run start:backups\"",
    "start:main": "cross-env NODE_ENV=development electron -r ts-node/register/transpile-only -r tsconfig-paths/register ./src/apps/main/main.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config .erb/configs/webpack.config.renderer.dev.ts",
    "start:sync-engine": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config .erb/configs/webpack.config.sync-engine.ts",
    "start:backups": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config .erb/configs/webpack.config.backups.ts",
    "========== Testing ==========": "",
    "test": "vitest",
    "test:once": "npm run test -- --run",
    "test:one": "npm run test -- related x",
    "test:cov": "npm run test:once -- --coverage",
    "test:e2e": "playwright test --config=tests",
    "test:all": "npm run test:once && npm run test:e2e",
    "========== Native deps ==========": "",
    "electron-rebuild": "electron-rebuild --sequential --force --types prod,dev,optional --module-dir .",
    "reload-native-deps": "ts-node .erb/scripts/check-native-deps.ts && npm run electron-rebuild && pnpm install",
    "========== Other ==========": "",
    "prepare": "husky install",
    "generate_schema": "openapi-typescript http://localhost:3004/api-json -o ./src/apps/shared/HttpClient/schema.d.ts"
  },
  "build": {
    "productName": "Internxt",
    "appId": "com.internxt.drive",
    "asar": true,
    "asarUnpack": [
      "**\\*.{node,dll}"
    ],
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "verifyUpdateCodeSignature": false
    },
    "directories": {
      "buildResources": "assets",
      "output": "build"
    },
    "extraResources": [
      "./assets/**",
      "./clamAV/**"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
