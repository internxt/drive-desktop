{
  "name": "internxt-drive",
  "version": "2.6.6",
  "author": "Internxt <hello@internxt.com>",
  "description": "Internxt client UI",
  "main": "./dist/main/main.js",
  "license": "MIT",
  "scripts": {
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\" \"npm run build:preload\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config .erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config .erb/configs/webpack.config.renderer.prod.ts",
    "build:preload": "npx esbuild src/apps/main/preload.ts --outfile=src/apps/main/preload.js --bundle --platform=node --format=cjs --external:electron --external:electron-log --external:path --external:@internxt",
    "build:dll": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config .erb/configs/webpack.config.renderer.dev.dll.ts",
    "package": "rimraf dist && npm run build && electron-builder build --publish never",
    "init:ci": "npm ci --ignore-scripts && node node_modules/electron/install.js",
    "init:dev": "npm install && node node_modules/electron/install.js && npm run build:dll && npm run reload-native-deps",
    "========== Code style ==========": "",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 276",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier src tests --check",
    "format:fix": "prettier src tests --write",
    "type-check": "tsc",
    "find-deadcode": "knip",
    "========== Start ==========": "",
    "start": "npm run start:renderer",
    "start:nodemon": "nodemon",
    "start:reload": "npm run build:preload && npm run start:main",
    "start:main": "cross-env NODE_ENV=development electron .erb/scripts/electron-launcher.js",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config .erb/configs/webpack.config.renderer.dev.ts",
    "========== Testing ==========": "",
    "test": "vitest",
    "test:one": "vitest --watch related x",
    "test:infra": "vitest --config vitest.config.infra.mts",
    "test:infra:one": "vitest --config vitest.config.infra.mts --watch related x",
    "test:renderer": "vitest --config vitest.config.renderer.mts",
    "test:renderer:one": "vitest --config vitest.config.renderer.mts --watch related x",
    "coverage:merge": "lcov-result-merger 'coverage/*/lcov.info' 'coverage/lcov.info'",
    "========== Native deps ==========": "",
    "electron-rebuild": "electron-rebuild --sequential --force --types prod,dev,optional --module-dir .",
    "reload-native-deps": "tsx .erb/scripts/check-native-deps.ts && npm run electron-rebuild",
    "========== Other ==========": "",
    "clamav": "powershell release/clamAV.ps1",
    "prepare": "husky install",
    "swagger_dev": "openapi-typescript http://localhost:3004/api-json -o ./src/apps/shared/HttpClient/schema.ts",
    "swagger_prod": "openapi-typescript https://gateway.internxt.com/drive/api-json -o ./src/apps/shared/HttpClient/schema.ts"
  },
  "build": {
    "productName": "Internxt",
    "appId": "com.internxt.drive",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "protocols": {
      "name": "Internxt",
      "schemes": [
        "internxt"
      ]
    },
    "win": {
      "target": "nsis",
      "verifyUpdateCodeSignature": false,
      "artifactName": "Internxt-Setup-${version}.${ext}"
    },
    "directories": {
      "buildResources": "assets",
      "output": "build"
    },
    "extraResources": [
      "./assets/**",
      "./clamAV/**"
    ]
  },
  "devDependencies": {
    "@internxt/eslint-config-internxt": "1.0.9",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@svgr/webpack": "^6.1.2",
    "@tanstack/eslint-plugin-query": "^4.43.0",
