{
  "name": "internxt",
  "version": "2.5.2",
  "author": "Internxt <hello@internxt.com>",
  "description": "Internxt Drive client UI",
  "main": "./dist/main/main.js",
  "license": "AGPL-3.0",
  "repository": {
    "url": "https://github.com/internxt/drive-desktop-linux"
  },
  "scripts": {
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir .",
    "reinstall:nautilus-extension": "NODE_ENV=development ts-node src/apps/nautilus-extension/reload.ts",
    "lint": "cross-env NODE_ENV=development eslint . --ext .ts,.tsx --max-warnings=235",
    "lint:fix": "npm run lint --fix",
    "format": "prettier src --check",
    "format:fix": "prettier src --write",
    "package": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build --publish never",
    "publish": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build --publish always",
    "postinstall": "electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts && opencollective-postinstall",
    "start": "ts-node ./.erb/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electron -r ts-node/register/transpile-only ./src/apps/main/main.ts",
    "start:main:debug": "cross-env NODE_ENV=development electron --inspect=5858 -r ts-node/register/transpile-only ./src/apps/main/main.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "test": "concurrently \"npm:test:main\" \"npm:test:renderer\"",
    "test:main": "vitest --config vitest.config.main.ts",
    "test:main:watch": "vitest --config vitest.config.main.ts --watch",
    "test:main:coverage": "vitest --config vitest.config.main.ts --coverage",
    "test:renderer": "vitest --config vitest.config.renderer.ts",
    "test:renderer:coverage": "vitest --config vitest.config.renderer.ts --coverage",
    "test:coverage": "concurrently \"npm:test:main:coverage\" \"npm:test:renderer:coverage\"",
    "coverage:merge": "lcov-result-merger 'coverage/*/lcov.info' 'coverage/lcov.info'",
    "type-check": "./scripts/tsc-max-errors.sh",
    "prepare": "husky install",
    "generate_schema": "openapi-typescript http://localhost:3005/api-json -o ./src/infra/schemas.d.ts",
    "generate_schema:prod": "openapi-typescript https://gateway.internxt.com/drive/api-json -o ./src/infra/schemas.d.ts",
    "find-deadcode": "knip --max-issues=206"
  },
  "build": {
    "productName": "Internxt",
    "beforeBuild": "./beforeBuild.js",
    "appId": "com.internxt.drive",
    "asar": true,
    "asarUnpack": [
      "**\\*.{node,dll}",
      "**/node_modules/sharp/**"
    ],
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "protocols": [
      {
        "name": "Internxt Login Protocol",
        "schemes": [
          "internxt"
        ]
      }
    ],
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Development"
    },
    "deb": {
      "depends": [
        "libfuse2",
        "python3-nautilus"
      ]
    },
    "directories": {
      "buildResources": "assets",
      "output": "build"
    },
    "extraResources": [
      "./assets/**",
      "./clamAV/**",
      "./src/apps/nautilus-extension/internxt-virtual-drive.py"
    ],
    "publish": {
      "provider": "github"
    }
  },
  "devDependencies": {
    "@internxt/eslint-config-internxt": "^1.0.9",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.4",
    "@svgr/webpack": "^6.1.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/async": "^3.2.16",
    "@types/bytes": "^3.1.1",
    "@types/chance": "^1.1.3",
    "@types/crypto-js": "^4.2.0",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.178",
    "@types/node": "22.0.2",
    "@types/node-schedule": "^2.1.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/terser-webpack-plugin": "^5.0.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.1",
    "axios-mock-adapter": "^1.21.2",
    "chalk": "^4.1.2",
    "concurrently": "^6.5.1",
    "core-js": "^3.20.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "detect-port": "^1.3.0",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "electron": "^19.1.9",
    "electron-builder": "^23.6.0",
    "electron-notarize": "^1.1.1",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.27.5",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "knip": "^5.80.0",
    "lcov-result-merger": "^5.0.1",
    "mini-css-extract-plugin": "^2.4.5",
    "ms": "^2.1.3",
    "openapi-typescript": "^7.6.1",
    "opencollective-postinstall": "^2.0.3",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "react-refresh": "^0.11.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.3.3",
    "terser-webpack-plugin": "^5.3.0",
    "ts-essentials": "^10.1.1",
    "ts-loader": "^9.2.6",
